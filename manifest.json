{
    "manifest_version": 2,
    "name": "word Definition",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "./1.jpeg",
        "default_popup": "sketch/index.html",
        "default_title": "Popup for word definition."
    }
}